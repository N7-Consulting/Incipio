{# Layout principal pour la gestion des documents #}
{% extends "base.html.twig" %}



{% block content %}
    <div class="box box-primary">
        <div class="box-header with-border">
            <h3 class="box-title">
                {% block content_title %}
                   
                {% endblock %}
            </h3>

           
        </div>

        <div class="box-body">
            {% block content_bundle %}
            {% endblock %}
        </div>

        <div class="box-footer"></div>
    </div>
{% endblock %}
