{# Use these templates to display information throughout all Tabs (Négociation, Acceptées, en Cours...) #}

{% if etude.ccaActive or etude.bdc is not none %}
	{% if getEtatDoc(etude.bdc) == 2 %}
		<img src="{{ valide }}"/>
	{% elseif getEtatDoc(etude.bdc) == 1 %}
		<img src="{{ enCours }}"/>
	{% else %}
		<img src="{{ nonValide }}"/>
	{% endif %}
{% endif %}
