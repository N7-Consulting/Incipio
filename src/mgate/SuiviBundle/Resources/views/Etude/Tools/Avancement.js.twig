$('body').click(function(e){
    var Elem = e.target;
    if (Elem.id == 'avancement'){
        bootbox.prompt("Avancement du developpeur (%) ?", function(result) {
        if (result !== null) {
            var requete = $.ajax({
                url: "{{ path('mgateSuivi_mission_avancement') }}",
                type: "POST",
                data: "avancement=" + result +"&id=" + $(Elem).data("etude") + "&intervenant=" + $(Elem).data("mission"),
                success: function(){},
            });
            $("#avancementBar"+$(Elem).data("title")+$(Elem).data("content")).css("width",  result + "%");
        }
    })
}});
